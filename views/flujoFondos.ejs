<div class="container my-3"> 
  <h1>FLUJO DE FONDOS</h1>

  <!--TABLA-->         

  <div class="card border-0">
    <div class="card-header">
        <h5 class="card-title">
            Basic Table
        </h5>
        <h6 class="card-subtitle text-muted">
            Lorem ipsum dolor sit amet consectetur adipisicing elit. Voluptatum ducimus,
            necessitatibus reprehenderit itaque!
        </h6>
    </div>
    <div class="card-body">

      <table  id="tabla-flujoFondos" class="table table-striped table-ligth table-bordered table-hover">
          <thead>
            <tr>
              <th>Nro.</th>
              <th>Mes</th>
              <th>AÃ±o</th>
              <th>T.Ingresos</th>
              <th>T.Inversiones</th>
              <th>T.Egresos</th>
              <th>Total</th>
              <th>Acumulado</th>
              <th>Fecha</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <% flujoFondos.forEach(function(item) { %>
              <td><%= item.idFlujoFondos %></td>
              <td><%= item.mes %></td>
              <td><%= item.year %></td>
              <td><%= item.ingresos %></td>
              <td><%= item.inversiones %></td>
              <td><%= item.egresos %></td>
              <td><%= item.total %></td>
              <td><%= item.acumulado %></td>
              <td><%= item.fecha %></td>
            </tr>
            <% }); %>
          </tbody>
      </table>
    </div>
</div>